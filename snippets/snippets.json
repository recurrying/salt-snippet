      "<Icon",
      "\tclassName=\"$1\"",
      "\tname=\"$2\"",
      "\tfill=\"#000\"",
      "\twidth=\"32px\"",
      "\theight=\"32px\"",
      "\tonClick={}",
      "/>"
    ],
    "description": "SaltUI Icon"
  },
