  },
  "Group Head": {
    "prefix": "salt-group-head",
    "body": [
      "<Group.Head",
      "\tclassName=\"$2\"",
      ">",
      "\t$1",
      "</Group.Head>"
    ],
    "description": "SaltUI Group Head"
  },
  "Group List": {
    "prefix": "salt-group-list",
    "body": [
      "<Group.List",
      "\tclassName=\"$2\"",
      "\tlineIndent={}",
      "\titemIndent={}",
      "\tborderTopNone={false}",
      ">",
      "\t$1",
      "</Group.List>"
    ],
    "description": "SaltUI Group List"
  },
  "Crumb": {
    "prefix": "salt-crumb",
    "body": [
      "<Crumb",
      "\tclassName=\"$2\"",
      "\tshowScroll={true}",
